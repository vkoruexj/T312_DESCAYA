class Voter
{
String name;
String dob;
int id;
Integer age;
//boolean card;
Voter(String names , String dobs , int ids , Integer ages)
{
name = names;
dob = dobs;
age = ages;
if(age > = 18)
{
id= ids;
}
// card = cards;
}
class Voter_Card
{
boolean status;
String votername;
int voterid;
Voter_Card(String voternames , int votersid , boolean statuss)
{
votername = voternames ;
voterid = votersid ;
status = statuss;
}
}
public void display()
{
System.out.println(votername+ "\t" + voterid +"\t"+dob+"\t"+age+"\t"+status+"\t");
}
}
